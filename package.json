{
  "name": "fer-templates",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "commit": "czg",
    "dev": "vite",
    "dev2": "esno ./build/script/postBuild.ts",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{scss,less,styl,html}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "dependencies": {
    "crypto-js": "^4.2.0",
    "lodash-es": "^4.17.21",
    "pinia": "^3.0.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.1",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^22.14.0",
    "@typescript-eslint/parser": "^8.30.1",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "cz-git": "^1.11.1",
    "czg": "^1.11.1",
    "eslint": "^9.22.0",
    "eslint-plugin-vue": "~10.0.0",
    "esno": "^4.8.0",
    "jiti": "^2.4.2",
    "less": "^4.3.0",
    "lint-staged": "^15.5.1",
    "npm-run-all2": "^7.0.2",
    "postcss": "^8.5.3",
    "postcss-html": "^1.8.0",
    "postcss-less": "^6.0.0",
    "prettier": "3.5.3",
    "stylelint": "^16.18.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended": "^16.0.0",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-order": "^7.0.0",
    "terser": "^5.39.0",
    "typescript": "~5.8.0",
    "vite": "^6.2.4",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vue-tsc": "^2.2.8"
  }
}
